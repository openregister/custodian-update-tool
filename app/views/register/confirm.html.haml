Check the details of your new #{prepare_register_name(@register_name)}

#{params[:action]}
The key is #{params[:key]}

%table
  %thead
    %tr
      %th Fields
      - if @current_register_record.nil?
        %th Values
      - else
        %th Current values
        %th New Values
  %tbody
    - @register.fields.each do |field|
      %tr
        %td #{field}
        %td #{@current_register_record.instance_variable_get("@#{field.gsub('-', '_')}")}
        %td
          - if !(@current_register_record.instance_variable_get("@#{field.gsub('-', '_')}") == nil && params[field.to_sym] == '')
            - if @current_register_record.instance_variable_get("@#{field.gsub('-', '_')}") != params[field.to_sym]
              .panel.panel-change-notification #{params[field.to_sym]}

.notice
  %i.icon.icon-important
    %span.visually-hidden Warning
  %strong.bold-small
    All changes will be published to the Country register

= button_to "Submit", { action: "create", params: params, data_confirmed: true}, data: { accepted: true }
